<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:SalesMaster.ViewModel"
                    xmlns:v="clr-namespace:SalesMaster.View">

    <DataTemplate DataType="{x:Type vm:TopViewModel}">
        <v:Top/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:NavigationViewModel}">
        <v:Navigation/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:StateViewModel}">
        <v:State/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <v:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:NewViewModel}">
        <v:New/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SearchViewModel}">
        <v:Search/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CompanyViewModel}">
        <v:Company/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsViewModel}">
        <v:Settings/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SearchMainViewModel}">
        <v:SearchMain/>
    </DataTemplate>

    <!--<DataTemplate DataType="{x:Type vm:EditMainViewModel}">
        <v:EditMain/>
    </DataTemplate>-->

</ResourceDictionary>